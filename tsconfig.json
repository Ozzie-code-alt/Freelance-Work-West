{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/AdministrationOfficeandFinance/ClientFeedbackForm", "app/api/auth/[...nextauth]/route.ts", ".next/types/app/api/auth/[...nextauth]/route.ts", "app/api/generateComments/route.js", "app/CommentsPage/page.jsx"],
  "exclude": ["node_modules",".next"]
}
